{{collection.Item}} {{#each items}}
<div class="card" style="width: 18rem;">
    <a href={{imageUrl}} target="_blank">
         <img class="card-img-top" style="max-height: 300px; object-fit: cover;" src="{{imageUrl}}" alt="Card image cap">
    </a>
    <div class="card-body">
        <h5 class="card-title">{{name}}</h5>
        <p class="card-text">{{description}}</p>
    </div>
</div>
{{/each}}